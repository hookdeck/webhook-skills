# OpenAI API Configuration
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Webhook Configuration
# The webhook secret from OpenAI follows the Standard Webhooks format.
# Format: whsec_ followed by base64 encoded key (e.g., whsec_1a2b3c4d5e6f...)
# Get this from your OpenAI dashboard when configuring webhooks
OPENAI_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Server Configuration
PORT=8000